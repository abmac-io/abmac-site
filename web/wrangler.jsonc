{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "abmac-site",
  "compatibility_date": "2024-04-01",
  "main": "./src/index.tsx",
  "assets": {
    "directory": "./dist/client",
    "binding": "ASSETS",
    "run_worker_first": true,
    "html_handling": "auto-trailing-slash",
    "not_found_handling": "single-page-application"
  }
}
